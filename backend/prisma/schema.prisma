generator client {
  provider = "prisma-client-js"
  output   = "../src/generated/prisma"
}

datasource db {
  provider = "mongodb"
  url      = env("DATABASE_URL")
}

model Product {
  id    String @id @default(auto()) @map("_id") @db.ObjectId
  name  String
  price Float
}

model User {
  id    String @id @default(auto()) @map("_id") @db.ObjectId
  age   Int
  email String
  name  String
}
